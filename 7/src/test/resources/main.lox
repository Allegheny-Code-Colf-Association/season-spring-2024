fun modulus(number, divisor) {
  while (number >= 0) {
    number = number - divisor;
  }
  number = number + divisor;
  return number;
}

for (var i = 1; i <= 1000; i = i + 1) {
  var result = i;
  result=modulus(i,3)==0?"Fizz":result;
  if (modulus(i, 3) == 0) {
    result = "Fizz";
  }
  if (modulus(i, 5) == 0) {
    result = "Buzz";
  }
  if (modulus(i, 15) == 0) {
    result = "FizzBuzz";
  }
  print result;
}
